<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="fr.charline.advantcalendar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".christmas.VideoActivity" />
        <activity android:name=".christmas.WebActivity" />
        <activity android:name=".christmas.ImageActivity" />
        <activity android:name=".christmas.GifActivity" />
        <activity android:name=".christmas.ChristmasActivity" />
        <activity android:name=".newyear.NewYearActivity" />
    </application>

</manifest>